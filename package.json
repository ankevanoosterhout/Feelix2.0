{
  "name": "feelix",
  "productName": "Feelix",
  "version": "3.0.0",
  "main": "electron/main.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuild": "electron-rebuild -f -w serialport",
    "electron": "ng build --base-href ./ && electron .",
    "bobbie": "npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && electron . --serve",
    "package-mac": "electron-packager . --overwrite --platform=darwin --asar --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune --out=release-builds",
    "package-win": "electron-packager . feelix --overwrite --asar --platform=win32 --arch=x64 --icon=src/assets/icons/win/icon.ico --prune --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Feelix\"",
    "package-linux": "electron-packager . feelix --overwrite --asar --platform=linux --arch=x64 --icon=src/assets/icons/png/512x512.png --prune --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@tensorflow/tfjs": "^3.21.0",
    "@types/uuid": "^3.4.7",
    "@types/webgl2": "^0.0.6",
    "closed-chain-ik": "file:closed-chain-ik",
    "d3": "^7.6.1",
    "d3-color": "^3.1.0",
    "drag-controls": "^1.0.4",
    "electron-browser-storage": "^1.0.6",
    "file-saver": "^2.0.2",
    "file-system": "^2.2.2",
    "jszip": "^3.7.0",
    "ngx-color-picker": "^7.5.0",
    "ngx-electron": "^2.2.0",
    "ngx-filesaver": "^8.1.0",
    "ngx-fs": "0.0.1",
    "ngx-webstorage": "^4.0.2",
    "rxjs": "~6.4.0",
    "serialport": "^9.0.7",
    "three": "^0.138.3",
    "three-ik": "^0.1.0",
    "tslib": "^1.9.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/electron": "^1.6.10",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.10.59",
    "codelyzer": "^5.2.1",
    "electron": "^6.0.10",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^1.11.0",
    "electron-reload": "1.5.0",
    "electron-wix-msi": "^3.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "npm-run-all": "4.1.5",
    "protractor": "^5.4.3",
    "ts-node": "8.3.0",
    "tslint": "~5.15.0",
    "typescript": "3.5.3",
    "wait-on": "3.3.0"
  }
}
